{
  "name": "takuzu",
  "version": "0.14.0",
  "private": true,
  "scripts": {
    "serve": "vite",
    "dev": "vite",
    "dev:host": "vite --host",
    "build": " vite build",
    "build:alpha": "vite build --mode alpha",
    "build:beta": "vite build --mode beta",
    "deploy": "vite build && surge dist --domain https://takuzu-vue.surge.sh",
    "deploy:test": "vite build --mode beta && surge dist --domain https://takuzu-vue-test.surge.sh",
    "deploy:test2": "vite build --mode alpha && surge dist --domain https://takuzu-vue-test2.surge.sh",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "test": "run-s test:unit",
    "vitest": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.cjs,.mjs,.ts,.cts,.mts,.jsx,.tsx --fix --ignore-path .gitignore",
    "lint:src": "eslint src --ext .vue,.js,.cjs,.mjs,.ts,.cts,.mts,.jsx,.tsx --fix --ignore-path .gitignore",
    "lint:nofix": "eslint . --ext .vue,.js,.cjs,.mjs,.ts,.cts,.mts,.jsx,.tsx --ignore-path .gitignore"
  },
  "dependencies": {
    "@fontsource/hammersmith-one": "^4.5.1",
    "@fontsource/roboto": "^4.5.3",
    "@vueform/multiselect": "^2.3.3",
    "@vueuse/core": "^9.3.0",
    "chart.js": "^3.9.1",
    "chartjs-adapter-date-fns": "^2.0.0",
    "date-fns": "^2.29.3",
    "dexie": "^3.2.0",
    "dexie-export-import": "^1.0.3",
    "lodash.debounce": "^4.0.8",
    "pinia": "^2.0.22",
    "vue": "^3.2.40",
    "vue-router": "^4.1.5",
    "vue-slider-component": "^4.0.0-beta.4"
  },
  "devDependencies": {
    "@iconify/json": "^2.1.115",
    "@rushstack/eslint-patch": "^1.2.0",
    "@tailwindcss/forms": "^0.5.3",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^18.7.23",
    "@types/tailwindcss": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.1.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.24.0",
    "eslint-plugin-vue": "^9.5.1",
    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "rollup-plugin-copy": "^3.4.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.4",
    "unplugin-icons": "^0.14.11",
    "unplugin-vue-components": "^0.22.7",
    "vite": "^3.1.4",
    "vite-plugin-pwa": "^0.13.1",
    "vitest": "^0.23.4",
    "vue-tsc": "^0.40.13"
  }
}
