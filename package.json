{
  "name": "takuzu",
  "version": "0.14.0",
  "private": true,
  "type": "module",
  "scripts": {
    "serve": "vite",
    "dev": "vite",
    "dev:host": "vite --host",
    "build": " vite build",
    "build:alpha": "vite build --mode alpha",
    "build:beta": "vite build --mode beta",
    "deploy": "vite build && surge dist --domain https://takuzu-vue.surge.sh",
    "deploy:test": "vite build --mode beta && surge dist --domain https://takuzu-vue-test.surge.sh",
    "deploy:test2": "vite build --mode alpha && surge dist --domain https://takuzu-vue-test2.surge.sh",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "test": "run-s test:unit",
    "vitest": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.cjs,.mjs,.ts,.cts,.mts,.jsx,.tsx --fix --ignore-path .gitignore",
    "lint:src": "eslint src --ext .vue,.js,.cjs,.mjs,.ts,.cts,.mts,.jsx,.tsx --fix --ignore-path .gitignore",
    "lint:nofix": "eslint . --ext .vue,.js,.cjs,.mjs,.ts,.cts,.mts,.jsx,.tsx --ignore-path .gitignore"
  },
  "dependencies": {
    "@fontsource/hammersmith-one": "^5.0.8",
    "@fontsource/roboto": "^5.0.8",
    "@vueform/multiselect": "^2.6.6",
    "@vueuse/core": "^10.7.1",
    "chart.js": "^4.4.1",
    "chartjs-adapter-date-fns": "^3.0.0",
    "date-fns": "^3.2.0",
    "dexie": "^3.2.4",
    "dexie-export-import": "^4.0.7",
    "lodash.debounce": "^4.0.8",
    "pinia": "^2.1.7",
    "vue": "^3.4.7",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.166",
    "@rushstack/eslint-patch": "^1.6.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@tsconfig/node20": "^20.1.2",
    "@types/jsdom": "^21.1.6",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^20.10.7",
    "@types/seedrandom": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@vitejs/plugin-vue": "^5.0.2",
    "@vitest/coverage-v8": "^1.1.3",
    "@vitest/ui": "^1.1.3",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "jsdom": "^23.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.33",
    "seedrandom": "^3.0.5",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.3.3",
    "unplugin-icons": "^0.18.2",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.11",
    "vite-plugin-pwa": "^0.17.4",
    "vite-plugin-static-copy": "^1.0.0",
    "vitest": "^1.1.3",
    "vue-tsc": "^1.8.27"
  }
}
