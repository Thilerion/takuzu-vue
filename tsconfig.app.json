{
	"extends": "@vue/tsconfig/tsconfig.web.json",
	"include": ["env.d.ts", "components.d.ts", "src/**/*.ts", "src/**/*.js", "src/**/*.vue"],
	"exclude": ["src/**/__tests__/*"],
	"compilerOptions": {
	  "composite": true,
	  "baseUrl": ".",
	  "paths": {
		"@/*": ["./src/*"]
	  },
	  "allowJs": true,
	  "types": [
		"vite-plugin-pwa/client"
	  ],
	  "lib": [
		"ES2016",
		"ES2018.Intl",
		"DOM",
		"DOM.Iterable",
		"ES2019.Array",
		"ES2017.String",
		"ES2020.String"
	  ]
	}
  }
  